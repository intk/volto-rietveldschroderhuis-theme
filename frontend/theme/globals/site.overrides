      -o-object-position: right !important;
      object-position: right !important;
    }

    .header-title-dates {
      width: 50%;
      bottom: 32%;
      padding-left: 100px;
      padding-right: 100px;

      .hero-title-floating,
      .description-container,
      .hero-dates {
        color: #000000 !important;
      }
    }
  }
    @media (min-width: 992px) and (max-width: 1300px) {
      .hero-title-floating{
