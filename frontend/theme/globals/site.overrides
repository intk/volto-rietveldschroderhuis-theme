body {
  overflow-x: hidden;
  -webkit-tap-highlight-color: #0085a1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-family: 'FranklinBook', Arial, sans-serif;
  font-size: 16px;
  color: #404040;
}
// @media screen and (min-width: 1200px) {
//   .view-viewview .ui.basic.segment.content-area {
//     margin-left: 100px;
//   }
// }

html {
  scroll-behavior: smooth;
}

#page-document.ui.container, #page-edit .ui.container{
  width: @container-width !important;
}
@media screen and (max-width: 768px){
  #page-document.ui.container, #page-edit.ui.container {
    width: 100% !important;
    margin: 0% !important;
    padding-right: 0% !important;
    padding-left: 0% !important;
  }
  #page-document.ui.container > *:not(.block.image.align, .block.video.align), #page-edit.ui.container >*:not(.block.image.align, .block.video.align){
    padding-left: 30px;
    padding-right: 30px;
  }
  #page-document.ui.container > ol, #page-document.ui.container > ul {
    padding-left: 70px !important;
  }
}

p {
  line-height: 1.5;
  margin: 30px 0;
}

#view, #page-edit, #page-search{
  ::-moz-selection {
    color: white;
    text-shadow: none;
    background: @brand-primary;
  }

  ::selection {
    color: white;
    text-shadow: none;
    background: @brand-primary;
  }

  img::selection {
    color: white;
    background: transparent;
  }

  img::-moz-selection {
    color: white;
    background: transparent;
  }

  .image-wrapper-, .image-wrapper-large, .image-wrapper-medium, .image-wrapper-small, .block.video.align{
    margin-top: 0px;
    margin-bottom: 70px;
  }

  a {
    text-decoration: none;
    border-bottom: 1px solid black;
    color: @brand-primary-text;
  }

  a:hover,
  a:focus {
    color: @brand-primary-red;
    border-bottom: 1px solid @brand-primary-red !important;
  }

  h1,
  h2,
  h3,
  h4,
  p {
    font-family: 'FranklinBook', Arial, sans-serif;
    color: @brand-primary;
  }

  h1.documentFirstHeading {
    color: #282931;
    font-family: 'FranklinDmCp', Arial, sans-serif;
    font-size: 80px;
    font-weight: 400;
    text-transform: none;
    letter-spacing: 0.00em;
    line-height: 62px;
    max-width: 100%;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 18px;
    margin-top: 15px;
    border-bottom: 0px;
    text-align: center;
  }
  .documentFirstHeading::before {
    display: none;
  }
  @media screen and (max-width: 678px) {
    h1.documentFirstHeading {
      font-size: 40px;
      line-height: 35px;
      text-align: left;
    }
  }

  // Description
  .documentDescription:not(:empty) {
    color: #282931;
    font-size: 26.5px;
    letter-spacing: 0.00em;
    line-height: 42px;
    display: block;
    margin-top: 77px;
    margin-bottom: 40px;
    max-width: 100%;
    margin-left: auto;
    margin-right: auto;
    text-align: left;
    font-family: 'FranklinBook', Arial, sans-serif;

    @media screen and (max-width: 768px) {
      margin-top:30px;
      font-size: 24px;
    }
  }
  //

  h2{
    font-size: 30px;
    font-family: 'FranklinBook', Arial, sans-serif;
  }

  h3{
    font-family: 'FranklinMed', Arial, sans-serif;
    font-size: 25px;
    color: #494A51;
    letter-spacing: 0.0em;
    line-height: 32px;
    font-weight: 400;
    margin-top: 45px;
    margin-bottom: 10px;
    text-transform: none;
  }
  @media screen and (max-width: 1200px){
    h3{
      margin-top: 20px;
    }
  }
  @media screen and (max-width: 768px) {
    h3{
      font-size: 21.6px;
      line-height: 32px;
    }
  }


  p {
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 19.68px;
    line-height: 36px;
    letter-spacing: 0.0em;
    color: #494a51;
    margin: 8px 0;
    margin-bottom: 35px;
  }

  @media screen and (max-width: 768px) {
    p {
      font-size: 18.88px;
    }
  }

  #page-document a {
    border-bottom: 1px solid #494a51;
  }
  #page-document .image-wrapper- a {
    border-bottom: 0px !important;
  }

  blockquote {
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 19.68px;
    line-height: 36px;
    letter-spacing: 0.0em;
    color: #494a51 !important;
    margin: 8px 0;
    margin-bottom: 35px;
    background: #eee !important;
    padding: 1em;
    border-left: 1em solid #ccc;
    clear: both;
  }

  ol, ul{
    font-size: 18px;
    line-height: 37px;
    color: #494a51;
    padding-left: 40px;

    li{
      padding-left: 10px;
    }
  }
}


// Logo
.logo-nav-wrapper{
  display: none;
  position: relative;
  left: -180px;
  top: 50px;
  a{
    pointer-events: auto;
  }
 }

