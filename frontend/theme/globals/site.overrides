// About the size of the Images and the Videos on wide screens
@media screen and (min-width: 1200px) {
  .view-viewview .block.align.center {
    text-align: center;

    iframe,
    img:not(.placeholder,.medium,.small),
    .video-inner{
      max-width: 130%;
      margin-right: auto;
      margin-left: 50%;
      transform: translateX(-61.5%);

      // &.medium {
      //   max-width: 50%;
      // }

      // &.small {
      //   max-width: 25%;
      // }
    }

    iframe {
      width: 130%;
    }

    .video-inner {
      width: 930px;
      margin-left: 50%;
      transform: translateX(-61.5%);
    }
  }

  .block.image.align.center{
    padding-top: 45px !important;
  }

  .view-viewview .columns-view .block.image.align.center {
    img {
      max-width: 100% !important;
    }
  }
}



// image link section
#view #page-document image-wrapper a{
  border-bottom: none;
}

//image credits section
.view-viewview {
  position: relative;
  .block.video.align{
    #photo-credit {
      position: absolute;
      left: -30%;
    }
  }
  .block.image.align.center{
    .image-wrapper-large,
    .image-wrapper- {
      #photo-credit {
        position: absolute;
        left: -30%;
      }
    }
    .image-wrapper-medium {
      #photo-credit {
        position: absolute;
        left: 25%;
      }
    }
    .image-wrapper-small {
      #photo-credit {
        position: absolute;
        left: 38%;
      }
    }
  }
  @media screen and (max-width: 1200px) {
    //image credits section
    .block.video.align{
      #photo-credit {
        position: absolute;
        left: 0%;
      }
    }
    .block.image.align.center {
    .image-wrapper-large,
    .image-wrapper- {
      #photo-credit {
        position: absolute;
        left: 0%;
      }
    }
  }
  }
  @media screen and (max-width: 768px) {
    //image credits section
    .block.video.align {
      #photo-credit {
        position: absolute;
        left: 5%;
      }
    }

    .block.video.align, .block.image.align.center {
    .image-wrapper-large,
    .image-wrapper- {
      #photo-credit {
        position: absolute;
        left: 5%;
      }
    }
  }
}
