// hero title over
.herosection{
  height: 140px;
}

.has-hero-image .herosection,
.herosection img {
  height: 90vh;
  // height: 720px;
  min-height: 400px;
  max-height: 720px;

  @media (min-width: 1640px) {
      max-height: 1000px !important;
    }
}

.herosection-content-image {
  position: relative;
  z-index: 0;
  z-index: 2;
  display: flex;
  width: 100%;
  height: 90vh;
  min-height: 400px;
  max-height: 720px;
  align-items: flex-end;
  margin: 0;

  @media (min-width: 1640px) {
      max-height: 1000px !important;
    }

  picture {
    display: flex;
    width: 100%;
    height: 100%;
    align-items: center;
    justify-content: center;
  }

  img {
    min-width: 100%;
    min-height: 100%;
    object-fit: cover;
    object-position: center;
  }

  figcaption {
    position: absolute;
    z-index: 3;
    top: 100%;
    right: 0;
    padding-top: 4px;
    padding-right: 10px;
    padding-bottom: 4px;
    padding-left: 10px;
    margin-right: 0;
    background: transparent;
    caption-side: bottom;
    font-size: 14px;
    line-height: 18px;
    line-height: 2;
    opacity: 1;
    text-align: right;
  }

  &::after {
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    background-image: linear-gradient(to top,
        rgba(0, 0, 0, 0.7) 0%,
        rgba(0, 0, 0, 0) 50%);
    background-repeat: repeat-x;
    content: '';
    opacity: 0.9;
  }

  .ui.container {
    z-index: 1;
  }

  h1 {
    z-index: 1;
    max-width: 842px;
    padding-right: 40px;
    padding-bottom: 20px;
    padding-left: 40px;
    border-bottom: 0;
    margin: 0 0 10px;
    margin-bottom: 10px;
    color: #fff;
    font-size: 80px;
    letter-spacing: 0;
    line-height: 1;
    text-align: left;
  }
}

.header-wrapper {
  position: absolute;
  z-index: 3;
}


.view-viewview.top-image-align-right {
  @media (min-width: 992px){
    .header-container {
        background: #fbf9f2 !important;
      }

    .herosection-content-image {
      width: 50%;
      margin-left: auto;
      &::after {
        background-image: none !important;
      }
    }

    .herosection-content-image img {
      -o-object-fit: contain !important;
      object-fit: contain !important;
      -o-object-position: right !important;
      object-position: right !important;
    }

    .header-title-dates {
      width: 50%;
      bottom: 32%;
      padding-left: 100px;
      padding-right: 100px;

      .hero-title-floating,
      .description-container,
      .hero-dates {
        color: #000000 !important;
      }
    }
  }
    @media (min-width: 992px) and (max-width: 1300px) {
      .hero-title-floating{
        font-size: 5.5vw !important;
        line-height: 0.8em;
      }
      .description-container{
        font-size: 1.5vw;
      }
    }

}

